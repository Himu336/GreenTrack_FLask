{
    "version": 2,
    "builds": [
        {
            "src": "application.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.10.16"  
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "application.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.10.16"  
    }
}
